   eq Stmt.getIndent() {
    	ASTNode parent = getParent();
    	if ( parent != null ){
        	ASTNode gparent = parent.getParent();
    	    return (gparent instanceof Program) ? "" : parent.getIndent() + ASTNode.INDENT_TAB;
        }
        return "" ;
    }
