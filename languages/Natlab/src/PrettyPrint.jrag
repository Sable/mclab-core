    eq CompilationUnits.getPrettyPrintedLessComments() {
        StringBuffer buf = new StringBuffer();

        boolean first = true;
        for( Program p : getPrograms() ){
            if( first )
                first = false;
            else
                buf.append("\n==========================\n\n");
            
            buf.append( p.getPrettyPrinted() );
        }
        return buf.toString();
    }

